    "build:prod": "nx build --prod",
    "test:nur": "nx test nurse-cron --code-coverage",
    "test:azc": "nx test imng-az-func-configr --code-coverage",
    "test:ivtr": "nx test imng-inventory --code-coverage",
    "test:libs": "npm run test:data-entry && npm run test:odata && npm run test:grid-odata && npm run test:chart-odata && npm run test:grid && npm run test:type-ahead && npm run test:auth0-oidc && npm run test:app-insight && npm run test:ngrx-utils && npm run test:nrsrx-utils && npm run test:signalr && npm run test:ngrx-idle",
