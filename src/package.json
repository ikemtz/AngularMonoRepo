    "test:type-ahead": "ng test imng-ngxb-typeahead --code-coverage",
    "test:auth0-oidc": "ng test imng-auth0-oidc --code-coverage",
    "test:app-insight": "ng test imng-application-insights-ngrx --code-coverage",
    "test:ngrx-utils": "ng test imng-ngrx-utils --code-coverage",
    "test:nrsrx-utils": "ng test imng-nrsrx-client-utils --code-coverage",
