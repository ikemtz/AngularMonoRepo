    "build:odata": "rm -dr ./dist/libs/imng-kendo-odata & nx build imng-kendo-odata --prod --with-deps",
    "build:grid": "rm -dr ./dist/libs/imng-kendo-grid & nx build imng-kendo-grid --prod --with-deps",
