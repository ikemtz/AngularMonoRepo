    "build:nrsrx-utils": "rm -dr ./dist/libs/imng-nrsrx-client-utils & nx build imng-nrsrx-client-utils --prod --with-deps",
    "build:theme-blue": "cd ./libs/imng-kendo-theme-blue && npm i && npm run build && find . -type d -name \"node_modules\" -exec rm -rf {} + && cp -r . ../../dist/libs/imng-kendo-theme-blue/ ",
    "build:signalr": "rm -dr ./dist/libs/imng-signalr-ngrx & nx build imng-signalr-ngrx --prod --with-deps",
    "build:core": "rm -dr ./dist/libs/imng-core & nx build imng-core --prod",
    "copyfiles:auth0-oidc": "mkdir -p ./dist/libs/imng-auth0-oidc/assets && cp -vf ./node_modules/oidc-client/dist/* ./libs/imng-auth0-oidc/src/assets/*.html ./dist/libs/imng-auth0-oidc/assets",
