{
  "name": "nurser-ui",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "test:nur": "ng test nurser-ui --code-coverage",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "gen:ts": "node ./gen-ts.js && npm run format:write",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "ng update @nrwl/workspace",
    "update:check": "ng update",
    "workspace-schematic": "nx workspace-schematic",
    "distribute:nur-cicd-scripts": "copyfiles -V 'nur.CiCd/*' dist",
    "build:nur": "ng build nurser-ui --sourceMap=true --statsJson",
    "build:nur-prod": "ng build nurser-ui --prod --sourceMap=true --configuration=production",
    "build:data-entry": "rimraf ./dist/libs/imng-kendo-data-entry && ng-packagr -p ./libs/imng-kendo-data-entry/package.json",
    "build:odata": "rimraf ./dist/libs/imng-kendo-odata && ng-packagr -p ./libs/imng-kendo-odata/package.json",
    "build:chart-odata": "rimraf ./dist/libs/imng-kendo-chart-odata && ng-packagr -p ./libs/imng-kendo-chart-odata/package.json",
    "build:grid-odata": "rimraf ./dist/libs/imng-kendo-grid-odata && ng-packagr -p ./libs/imng-kendo-grid-odata/package.json",
    "build:type-ahead": "rimraf ./dist/libs/imng-ngxb-typeahead && ng-packagr -p ./libs/imng-ngxb-typeahead/package.json",
    "build:auth0-oidc": "rimraf ./dist/libs/imng-auth0-oidc && ng-packagr -p ./libs/imng-auth0-oidc/package.json",
    "build:app-insight": "rimraf ./dist/libs/imng-application-insights-ngrx && ng-packagr -p ./libs/imng-application-insights-ngrx/package.json",
    "build:ngrx-utils": "rimraf ./dist/libs/imng-ngrx-utils && ng-packagr -p ./libs/imng-ngrx-utils/package.json",
    "build:nrsrx-utils": "rimraf ./dist/libs/imng-nrsrx-client-utils && ng-packagr -p ./libs/imng-nrsrx-client-utils/package.json",
    "copyfiles:auth0-oidc": "copyfiles -Vf ./node_modules/oidc-client/dist/oidc-client.min.js ./libs/imng-auth0-oidc/src/assets/*.html ./dist/libs/imng-auth0-oidc/assets",
    "publish:data-entry": "npm run build:data-entry && npm publish ./dist/libs/imng-kendo-data-entry",
    "publish:odata": "npm run build:odata && npm publish ./dist/libs/imng-kendo-odata",
    "publish:chart-odata": "npm run build:chart-odata && npm publish ./dist/libs/imng-kendo-chart-odata",
    "publish:grid-odata": "npm run build:grid-odata && npm publish ./dist/libs/imng-kendo-grid-odata",
    "publish:type-ahead": "npm run build:type-ahead && npm publish ./dist/libs/imng-ngxb-typeahead",
    "publish:auth0-oidc": "npm run build:auth0-oidc && npm run copyfiles:auth0-oidc && npm publish ./dist/libs/imng-auth0-oidc",
    "publish:app-insight": "npm run build:app-insight && npm publish ./dist/libs/imng-application-insights-ngrx",
    "publish:ngrx-utils": "npm run build:ngrx-utils && npm publish ./dist/libs/imng-ngrx-utils",
    "publish:nrsrx-utils": "npm run build:nrsrx-utils && npm publish ./dist/libs/imng-nrsrx-client-utils",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "nx": "nx"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.11",
    "@angular/common": "^8.2.11",
    "@angular/compiler": "^8.2.11",
    "@angular/core": "^8.2.11",
    "@angular/forms": "^8.2.11",
    "@angular/platform-browser": "^8.2.11",
    "@angular/platform-browser-dynamic": "^8.2.11",
    "@angular/router": "^8.2.11",
    "@fortawesome/angular-fontawesome": "^0.4.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.25",
    "@fortawesome/free-solid-svg-icons": "^5.11.2",
    "@microsoft/applicationinsights-web": "^2.3.0",
    "@ngrx/effects": "8.4.0",
    "@ngrx/entity": "8.4.0",
    "@ngrx/router-store": "8.4.0",
    "@ngrx/store": "^8.4.0",
    "@nrwl/angular": "8.6.0",
    "@progress/kendo-angular-buttons": "^5.0.0",
    "@progress/kendo-angular-common": "^1.1.0",
    "@progress/kendo-angular-dateinputs": "^4.1.0",
    "@progress/kendo-angular-dialog": "^4.1.0",
    "@progress/kendo-angular-dropdowns": "^4.2.0",
    "@progress/kendo-angular-excel-export": "^3.0.1",
    "@progress/kendo-angular-grid": "^4.4.0",
    "@progress/kendo-angular-inputs": "^6.3.0",
    "@progress/kendo-angular-intl": "^2.0.0",
    "@progress/kendo-angular-l10n": "^2.0.0",
    "@progress/kendo-angular-pdf-export": "^2.0.0",
    "@progress/kendo-angular-popup": "^3.0.1",
    "@progress/kendo-data-query": "^1.5.2",
    "@progress/kendo-drawing": "^1.6.0",
    "@progress/kendo-theme-default": "^4.5.0",
    "bootstrap": "^4.3.1",
    "core-js": "^2.6.10",
    "ngx-bootstrap": "^5.2.0",
    "oidc-client": "^1.9.1",
    "rxjs": "~6.5.3",
    "tslib": "^1.9.0",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.12",
    "@angular-devkit/build-ng-packagr": "~0.803.12",
    "@angular/cli": "^8.3.12",
    "@angular/compiler-cli": "^8.2.11",
    "@angular/language-service": "^8.2.11",
    "@ngrx/schematics": "8.4.0",
    "@ngrx/store-devtools": "8.4.0",
    "@nrwl/cypress": "8.6.0",
    "@nrwl/jest": "8.6.0",
    "@nrwl/workspace": "8.6.0",
    "@types/jest": "24.0.9",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.1.2",
    "copyfiles": "^2.1.1",
    "cypress": "^3.4.1",
    "dotenv": "6.2.0",
    "jest": "24.1.0",
    "jest-preset-angular": "^7.1.1",
    "ng-packagr": "^5.7.0",
    "ngrx-store-freeze": "0.2.4",
    "odata-ts-generator": "^0.1.26",
    "prettier": "1.16.4",
    "rimraf": "^2.7.1",
    "ts-jest": "24.0.0",
    "ts-node": "~7.0.0",
    "tsickle": "^0.35.0",
    "tslint": "~5.11.0",
    "typescript": "~3.5.3"
  }
}
