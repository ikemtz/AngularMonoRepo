    "lint:nur": "nx workspace-lint && nx lint nurse-cron",
    "lint:ivtr": "nx workspace-lint && nx lint imng-inventory",
    "lint:azc": "nx workspace-lint && nx lint imng-az-func-configr",
    "lint:libs": "nx workspace-lint && nx lint imng-application-insights-ngrx && nx lint imng-auth0-oidc && nx lint imng-kendo-chart-odata && nx lint imng-kendo-data-entry && nx lint imng-kendo-grid && nx lint imng-kendo-grid-odata && nx lint imng-kendo-odata && nx lint imng-ngrx-idle && nx lint imng-ngrx-utils && nx lint imng-ngxb-typeahead && nx lint imng-nrsrx-client-utils && nx lint imng-signalr-ngrx",
    "e2e": "nx e2e",
    "gen:ts:nrcrn": "node ./apps/nurse-cron/gen-ts.js && npm run format:write",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write --uncommitted",
    "format:check": "nx format:check",
    "update": "nx update @nrwl/workspace",
    "update:check": "nx update",
    "workspace-schematic": "nx workspace-schematic",
    "distribute:azc-cicd-scripts": "cp -v ./CiCd/azc/* ./dist",
    "distribute:ivtr-cicd-scripts": "cp -v ./CiCd/ivtr/* ./dist",
    "distribute:nur-cicd-scripts": "cp -v ./CiCd/nur/* ./dist",
    "build:libs": "npm run build:nrsrx-utils && npm run build:odata && npm run build:ngrx-utils && npm run build:data-entry && npm run build:app-insight && npm run build:auth0-oidc && npm run build:grid && npm run build:grid-odata && npm run build:type-ahead && npm run build:chart-odata && npm run build:signalr && npm run build:ngrx-idle && npm run build:theme-blue",
    "build:nur": "nx build nurse-cron --sourceMap=true --statsJson",
    "build:nur-prod": "nx build nurse-cron --prod --sourceMap=true --configuration=production",
    "build:azc": "nx build imng-az-func-configr --sourceMap=true --statsJson",
    "build:azc-prod": "nx build imng-az-func-configr --prod --sourceMap=true --configuration=production",
    "build:ivtr": "nx build imng-inventory --sourceMap=true --statsJson",
    "build:ivtr-prod": "nx build imng-inventory --prod --sourceMap=true --configuration=production",
    "build:data-entry": "rm -dr ./dist/libs/imng-kendo-data-entry & nx build imng-kendo-data-entry --prod --with-deps",
    "build:odata": "rm -dr ./dist/libs/imng-kendo-odata & nx build imng-kendo-odata --prod",
    "build:grid": "rm -dr ./dist/libs/imng-kendo-grid & nx build imng-kendo-grid --prod",
    "build:chart-odata": "rm -dr ./dist/libs/imng-kendo-chart-odata & nx build imng-kendo-chart-odata --prod",
    "build:grid-odata": "rm -dr ./dist/libs/imng-kendo-grid-odata & nx build imng-kendo-grid-odata --prod",
    "build:type-ahead": "rm -dr ./dist/libs/imng-ngxb-typeahead & nx build imng-ngxb-typeahead --prod",
    "build:auth0-oidc": "rm -dr ./dist/libs/imng-auth0-oidc & nx build imng-auth0-oidc --prod && npm run copyfiles:auth0-oidc",
    "build:app-insight": "rm -dr ./dist/libs/imng-application-insights-ngrx & nx build imng-application-insights-ngrx --prod",
    "build:ngrx-utils": "rm -dr ./dist/libs/imng-ngrx-utils & nx build imng-ngrx-utils --prod",
    "build:ngrx-idle": "rm -dr ./dist/libs/imng-ngrx-idle & nx build imng-ngrx-idle --prod",
    "build:nrsrx-utils": "rm -dr ./dist/libs/imng-nrsrx-client-utils & nx build imng-nrsrx-client-utils --prod",
    "build:theme-blue": "cd ./libs/imng-kendo-theme-blue && npm i && npm run build && find . -type d -name \"node_modules\" -exec rm -rf {} + && cp -r . ../../dist/libs/imng-kendo-theme-blue/ ",
    "build:signalr": "rm -dr ./dist/libs/imng-signalr-ngrx & nx build imng-signalr-ngrx --prod",
    "copyfiles:auth0-oidc": "mkdir -p ./dist/libs/imng-auth0-oidc/assets && cp -vf ./node_modules/oidc-client/dist/* ./libs/imng-auth0-oidc/src/assets/*.html ./dist/libs/imng-auth0-oidc/assets",
    "publish:libs": "npm run publish:data-entry && npm run publish:odata && npm run publish:chart-odata && npm run publish:grid && npm run publish:grid-odata && npm run publish:type-ahead && npm run publish:auth0-oidc && npm run publish:app-insight && npm run publish:ngrx-utils && npm run publish:nrsrx-utils && npm run publish:signalr && npm run publish:ngrx-idle && npm run publish:theme-blue",
    "publish:data-entry": "npm run build:data-entry && npm publish ./dist/libs/imng-kendo-data-entry",
    "publish:odata": "npm run build:odata && npm publish ./dist/libs/imng-kendo-odata",
    "publish:chart-odata": "npm run build:chart-odata && npm publish ./dist/libs/imng-kendo-chart-odata",
    "publish:grid": "npm run build:grid && npm publish ./dist/libs/imng-kendo-grid",
    "publish:grid-odata": "npm run build:grid-odata && npm publish ./dist/libs/imng-kendo-grid-odata",
    "publish:type-ahead": "npm run build:type-ahead && npm publish ./dist/libs/imng-ngxb-typeahead",
    "publish:auth0-oidc": "npm run build:auth0-oidc && npm run copyfiles:auth0-oidc && npm publish ./dist/libs/imng-auth0-oidc",
    "publish:app-insight": "npm run build:app-insight && npm publish ./dist/libs/imng-application-insights-ngrx",
    "publish:ngrx-utils": "npm run build:ngrx-utils && npm publish ./dist/libs/imng-ngrx-utils",
    "publish:ngrx-idle": "npm run build:ngrx-idle && npm publish ./dist/libs/imng-ngrx-idle",
    "publish:nrsrx-utils": "npm run build:nrsrx-utils && npm publish ./dist/libs/imng-nrsrx-client-utils",
    "publish:theme-blue": "npm run build:theme-blue && cd ./libs/imng-kendo-theme-blue && npm publish",
    "publish:signalr": "npm run build:signalr && npm publish ./dist/libs/imng-signalr-ngrx",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "nx": "nx",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points && node ./decorate-angular-cli.js",
    "schematic:module:emp": "nx g imng-schematics:module employee --path=./apps/nurse-cron/src/app/modules --openApiJsonUrl=https://im-wa-empo-nrcrn.azurewebsites.net/swagger/v1/swagger.json --force --appPrefix=nrcrn",
    "test": "snyk test"
  },
