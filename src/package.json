    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write --uncommitted",
    "format:check": "nx format:check",
    "update": "ng update @nrwl/workspace",
    "update:check": "ng update",
    "workspace-schematic": "nx workspace-schematic",
    "distribute:azc-cicd-scripts": "copyfiles -V 'CiCd/azc/*' dist",
    "distribute:nur-cicd-scripts": "copyfiles -V 'CiCd/nur/*' dist",
    "build:libs": "npm run build:odata && npm run build:data-entry && npm run build:auth0-oidc && npm run build:app-insight && npm run build:ngrx-utils && npm run build:nrsrx-utils && npm run build:grid && npm run build:grid-odata && npm run build:type-ahead && npm run build:chart-odata ",
    "build:nur": "ng build nurse-cron --sourceMap=true --statsJson",
    "build:nur-prod": "ng build nurse-cron --prod --sourceMap=true --configuration=production",
    "build:azc": "ng build imng-az-func-configr --sourceMap=true --statsJson",
    "build:azc-prod": "ng build imng-az-func-configr --prod --sourceMap=true --configuration=production",
    "build:data-entry": "rimraf ./dist/libs/imng-kendo-data-entry && ng build imng-kendo-data-entry --prod",
    "build:odata": "rimraf ./dist/libs/imng-kendo-odata && ng build imng-kendo-odata --prod",
    "build:grid": "rimraf ./dist/libs/imng-kendo-grid && ng build imng-kendo-grid --prod",
    "build:chart-odata": "rimraf ./dist/libs/imng-kendo-chart-odata && ng build imng-kendo-chart-odata --prod",
    "build:grid-odata": "rimraf ./dist/libs/imng-kendo-grid-odata && ng build imng-kendo-grid-odata --prod",
    "build:type-ahead": "rimraf ./dist/libs/imng-ngxb-typeahead && ng build imng-ngxb-typeahead --prod",
    "build:auth0-oidc": "rimraf ./dist/libs/imng-auth0-oidc && ng build imng-auth0-oidc --prod && npm run copyfiles:auth0-oidc",
    "build:app-insight": "rimraf ./dist/libs/imng-application-insights-ngrx && ng build imng-application-insights-ngrx --prod",
    "build:ngrx-utils": "rimraf ./dist/libs/imng-ngrx-utils && ng build imng-ngrx-utils --prod",
    "build:nrsrx-utils": "rimraf ./dist/libs/imng-nrsrx-client-utils && ng build imng-nrsrx-client-utils --prod",
    "copyfiles:auth0-oidc": "copyfiles -Vf ./node_modules/oidc-client/dist/oidc-client.min.js ./libs/imng-auth0-oidc/src/assets/*.html ./dist/libs/imng-auth0-oidc/assets",
    "publish:libs": "npm run publish:data-entry && npm run publish:odata && npm run publish:chart-odata && npm run publish:grid && npm run publish:grid-odata && npm run publish:type-ahead && npm run publish:auth0-oidc && npm run publish:app-insight && npm run publish:ngrx-utils && npm run publish:nrsrx-utils",
    "publish:data-entry": "npm run build:data-entry && npm publish ./dist/libs/imng-kendo-data-entry",
    "publish:odata": "npm run build:odata && npm publish ./dist/libs/imng-kendo-odata",
    "publish:chart-odata": "npm run build:chart-odata && npm publish ./dist/libs/imng-kendo-chart-odata",
    "publish:grid": "npm run build:grid && npm publish ./dist/libs/imng-kendo-grid",
    "publish:grid-odata": "npm run build:grid-odata && npm publish ./dist/libs/imng-kendo-grid-odata",
    "publish:type-ahead": "npm run build:type-ahead && npm publish ./dist/libs/imng-ngxb-typeahead",
    "publish:auth0-oidc": "npm run build:auth0-oidc && npm run copyfiles:auth0-oidc && npm publish ./dist/libs/imng-auth0-oidc",
    "publish:app-insight": "npm run build:app-insight && npm publish ./dist/libs/imng-application-insights-ngrx",
    "publish:ngrx-utils": "npm run build:ngrx-utils && npm publish ./dist/libs/imng-ngrx-utils",
    "publish:nrsrx-utils": "npm run build:nrsrx-utils && npm publish ./dist/libs/imng-nrsrx-client-utils",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "nx": "nx",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "schematic:module:unt": "ng g imng-schematics:module unit --path=\"./dist/\" --swaggerJsonUrl=https://im-wa-unto-nrcrn.azurewebsites.net/swagger/v1/swagger.json --dry-run=true --force --appPrefix=nrcrn"
  },
