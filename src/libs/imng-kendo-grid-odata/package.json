{
  "name": "imng-kendo-grid-odata",
  "version": "0.4.8",
  "author": "Isaac Martinez <@ikemtz>",
  "license": "MIT",
  "description": "A library to make integrating Angular Kendo Grid, OData endpoints, and NGRX a bit simpler.",
  "keywords": [
    "Angular",
    "NGRX",
    "Kendo",
    "Grid",
    "OData"
  ],
  "peerDependencies": {
    "imng-kendo-odata": "*",
    "@ngrx/store": ">=9.x",
    "tslib": ">=2.x",
    "@angular/core": ">=11.x",
    "@angular/common": ">=11.x",
    "@angular/platform-browser": ">=11.x",
    "@progress/kendo-angular-grid": ">=5.x",
    "imng-nrsrx-client-utils": "*",
    "@progress/kendo-data-query": ">=1.x",
    "rxjs": ">=6.x"
  },
  "dependencies": {
    "@nrwl/angular": ">=12.x"
  },
  "devDependencies": {
    "@nrwl/angular": ">=12.x"
  },
  "ngPackage": {
    "$schema": "./node_modules/ng-packagr/ng-package.schema.json",
    "lib": {
      "entryFile": "src/index.ts"
    },
    "dest": "../../dist/libs/imng-kendo-grid-odata",
    "allowedNonPeerDependencies": [
      "imng-kendo-odata",
      "@nrwl/angular"
    ]
  }
}
