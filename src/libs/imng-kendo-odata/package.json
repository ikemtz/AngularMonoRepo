{
  "name": "imng-kendo-odata",
  "version": "0.1.1",
  "description": "A library to make integrating Angular, Kendo controls, OData endpoints and NGRX a bit simpler.",
  "keywords": [
    "Angular",
    "NGRX",
    "Kendo",
    "OData"
  ],
  "peerDependencies": {
    "@progress/kendo-data-query": ">=1.x",
    "@angular/core": ">=11.x",
    "@angular/common": ">=11.x",
    "rxjs": ">=6.x",
    "@nrwl/angular": ">=11.x",
    "imng-nrsrx-client-utils": "*",
    "tslib": ">=2.x"
  },
  "devDependencies": {
    "ng-packagr": ">=12",
    "tslib": ">=2.x",
    "@nrwl/angular": ">=11.x"
  },
  "ngPackage": {
    "$schema": "./node_modules/ng-packagr/ng-package.schema.json",
    "lib": {
      "entryFile": "src/index.ts"
    },
    "dest": "../../dist/libs/imng-kendo-odata"
  },
  "repository": {
    "type": "git",
