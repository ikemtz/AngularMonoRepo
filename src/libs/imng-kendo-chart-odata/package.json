{
  "name": "imng-kendo-chart-odata",
  "version": "0.1.4",
  "author": "Isaac Martinez <@ikemtz>",
  "license": "MIT",
  "description": "A library to make integrating Angular Kendo Charts and NGRX a bit simpler.",
  "keywords": [
    "Angular",
    "NGRX",
    "Kendo",
    "Charts",
    "OData"
  ],
  "dependencies": {
    "imng-kendo-odata": "*"
  },
  "ngPackage": {
    "$schema": "./node_modules/ng-packagr/ng-package.schema.json",
    "lib": {
      "entryFile": "src/index.ts"
    },
    "dest": "../../dist/libs/imng-kendo-chart-odata",
    "whitelistedNonPeerDependencies": [
      "imng-kendo-odata"
    ]
  }
}
