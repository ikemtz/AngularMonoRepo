{
  "name": "imng-kendo-chart-odata",
  "version": "0.1.4",
  "author": "Isaac Martinez <@ikemtz>",
  "license": "MIT",
  "description": "A library to make integrating Angular Kendo Charts and NGRX a bit simpler.",
  "keywords": [
    "Angular",
    "NGRX",
    "Kendo",
    "Charts",
    "OData"
  ],
  "peerDependencies": {
    "@angular/common": ">=11.x",
    "@progress/kendo-data-query": ">=1.x",
    "@angular/core": ">=11.x",
    "rxjs": ">=6.x",
    "tslib": ">=2.x",
    "@nrwl/angular": ">=11.x"
  },
  "dependencies": { 
    "imng-kendo-odata": "*",
    "imng-nrsrx-client-utils": "*"
  },
  "ngPackage": {
    "$schema": "./node_modules/ng-packagr/ng-package.schema.json",
    "lib": {
      "entryFile": "src/index.ts"
    },
    "dest": "../../dist/libs/imng-kendo-chart-odata",
    "allowedNonPeerDependencies": [
      "imng-kendo-odata",
      "imng-nrsrx-client-utils"
    ]
  },
  "devDependencies": {
    "ng-packagr": ">=12.x"
  }
}
