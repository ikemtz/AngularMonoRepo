{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "typeRoots": ["node_modules/@types"],
    "lib": ["es2017", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@nurser-env/*": ["apps/nurser-ui/src/app/environments/*"],
      "imng-auth0-oidc": ["libs/imng-auth0-oidc/src/index.ts"],
      "imng-kendo-grid-odata": ["libs/imng-kendo-grid-odata/src/index.ts"],
      "imng-kendo-grid-odata/testing": ["libs/imng-kendo-grid-odata/testing/src/index.ts"],
      "imng-application-insights-ngrx": ["libs/imng-application-insights-ngrx/src/index.ts"],
      "imng-kendo-data-entry": ["libs/imng-kendo-data-entry/src/index.ts"],
      "imng-kendo-data-entry/testing": ["libs/imng-kendo-data-entry/testing/src/index.ts"],
      "imng-ngrx-utils": ["libs/imng-ngrx-utils/src/index.ts"],
      "imng-nrsrx-client-utils": ["libs/imng-nrsrx-client-utils/src/index.ts"],
      "imng-ngxb-typeahead": ["libs/imng-ngxb-typeahead/src/index.ts"],
      "imng-kendo-chart-odata": ["libs/imng-kendo-chart-odata/src/index.ts"],
      "imng-kendo-odata": ["libs/imng-kendo-odata/src/index.ts"],
      "@imng/imng-snippets": ["libs/imng-snippets/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
