{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "typeRoots": ["node_modules/@types"],
    "lib": ["es2017", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "imng-auth0-oidc": ["dist/libs/imng-auth0-oidc", "libs/imng-auth0-oidc/src"],
      "imng-kendo-grid-odata": ["dist/libs/imng-kendo-grid-odata", "libs/imng-kendo-grid-odata/src"],
      "imng-kendo-grid-odata/testing": [
        "dist/libs/imng-kendo-grid-odata/testing",
        "libs/imng-kendo-grid-odata/testing/src"
      ],
      "imng-application-insights-ngrx": [
        "dist/libs/imng-application-insights-ngrx",
        "libs/imng-application-insights-ngrx/src"
      ],
      "imng-kendo-odata": ["dist/libs/imng-kendo-odata", "libs/imng-kendo-odata/src"],
      "imng-kendo-data-entry": ["dist/libs/imng-kendo-data-entry", "libs/imng-kendo-data-entry/src"],
      "imng-kendo-data-entry/testing": [
        "dist/libs/imng-kendo-data-entry/testing",
        "libs/imng-kendo-data-entry/testing/src"
      ],
      "imng-ngrx-utils": ["dist/libs/imng-ngrx-utils", "libs/imng-ngrx-utils/src"],
      "imng-nrsrx-client-utils": ["dist/libs/imng-nrsrx-client-utils", "libs/imng-nrsrx-client-utils/src"],
      "imng-ngxb-typeahead": ["dist/libs/imng-ngxb-typeahead", "libs/imng-ngxb-typeahead/src"],
      "imng-kendo-chart-odata": ["dist/libs/imng-kendo-chart-odata", "libs/imng-kendo-chart-odata/src"],
      "imng-kendo-grid": ["dist/libs/imng-kendo-grid", "libs/imng-kendo-grid/src"],
      "imng-kendo-grid/testing": ["dist/libs/imng-kendo-grid/testing", "libs/imng-kendo-grid/testing/src"],
      "@imng/imng-schematics": ["libs/imng-schematics/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
